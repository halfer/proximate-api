        "slim/slim": "^3.0",
        "proximate/storage": "dev-master",
        "proximate/core": "dev-master",
        "proximate/crawler": "dev-master",
        "proximate/requester": "dev-master",
        "educoder/pest": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "mockery/mockery": "dev-master",
